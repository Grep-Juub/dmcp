{
  "name": "dmcp-server",
  "version": "2.0.0",
  "description": "Dynamic MCP Server - Query-driven tool discovery with Redis VSS and semantic search",
  "type": "module",
  "main": "dist/dmcp-server.js",
  "bin": {
    "dmcp-server": "dist/dmcp-server.js",
    "dmcp-indexer": "dist/dmcp-indexer.js"
  },
  "scripts": {
    "build": "tsc",
    "index": "REDIS_PORT=6380 tsx src/dmcp-indexer.ts",
    "index:force": "REDIS_PORT=6380 tsx src/dmcp-indexer.ts --force",
    "start": "REDIS_PORT=6380 tsx src/dmcp-server.ts",
    "start:prod": "npm run build && REDIS_PORT=6380 node dist/dmcp-server.js"
  },
  "keywords": [
    "mcp",
    "dmcp",
    "dynamic-mcp",
    "tool-discovery",
    "redis",
    "vector-search",
    "semantic-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@portkey-ai/mcp-tool-filter": "^1.0.1",
    "eventsource": "^4.1.0",
    "node-fetch": "^3.3.2",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
