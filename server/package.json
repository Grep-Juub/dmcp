{
  "name": "dmcp-server",
  "version": "3.0.0",
  "description": "Dynamic MCP Server - Query-driven tool discovery with Redis VSS and semantic search",
  "type": "module",
  "main": "dist/dmcp-server.js",
  "bin": {
    "dmcp-server": "dist/dmcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/dmcp-server.ts",
    "start:prod": "npm run build && node dist/dmcp-server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "dmcp",
    "dynamic-mcp",
    "tool-discovery",
    "redis",
    "vector-search",
    "semantic-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "express": "^4.21.0",
    "redis": "^5.10.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
